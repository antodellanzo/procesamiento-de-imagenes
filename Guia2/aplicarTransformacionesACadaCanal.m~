function [imagenHSI] = aplicarTransformacionesACadaCanal(imagen)
imagenHSI = rgbToHsi(imagen);

transformarH

figure;
subplot(1, 2, 1), imshow(imagenHSI), title('imagen en hsi');
subplot(1, 2, 
end

function [imagenHSI] = transformarH(imagenHSI)
H = imagenHSI(:, :, 1);
H = log(H + 1);
imagenHSI(:, :, 1) = H;
end

function [imagenHSI] = transformarS(imagenHSI)
S = imagenHSI(:, :, 2);
S = S * 2;
imagenHSI(:, :, 2) = S;
end

function [imagenHSI] = transformarI(imagenHSI)
I = imagenHSI(:, :, 3);
I = I / 2;
imagenHSI(:, :, 3) = I;
end